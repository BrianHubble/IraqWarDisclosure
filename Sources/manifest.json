{
    "manifest_version": 2,
    "name": "Iraq War Disclosure",
    "short_name": "IraqWarDisclosure",
    "description": "Reminds you of people who supported the Iraq War.",
    "author": "Brian Hubble",
    "version": "0.1",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "all_frames": true,
            "js": [
                "content_script.js"
            ],
            "run_at": "document_end"
        }
    ]
}